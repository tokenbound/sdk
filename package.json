{
  "name": "tokenbound-monorepo",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "clean": "rm -rf node_modules",
    "pretest": "rm -rf packages/sdk/src/test/bundler && git clone git@github.com:tokenbound/erc4337-bundler.git packages/sdk/src/test/bundler && cd packages/sdk/src/test/bundler && pnpm install && pnpm build",
    "test": "cd packages/sdk && pnpm test",
    "test:all": "pnpm pretest && cd packages/sdk && pnpm test && pnpm posttest",
    "posttest": "rm -rf packages/sdk/src/test/bundler",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "publishConfig.directory": {
    "directory": "packages/sdk"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^2.8.8",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "husky": "^8.0.3",
    "typescript": "^5.2.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "lint-staged": "^14.0.1"
  },
  "lint-staged": {
    "./**/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
